{
  "name": "calendar-react-ts",
  "version": "0.0.7",
  "description": "a calendar component by react with typescript",
  "main": "es/index.js",
  "scripts": {
    "start": "npm run dev",
    "build:css": "cd gulp && gulp compile",
    "build:commonjs": "rimraf lib && cross-env BABEL_ENV=commonjs babel src --extensions .ts,.tsx -d lib --source-maps && npm run build:css",
    "build:es": "rimraf es && cross-env BABEL_ENV=es babel src --extensions .ts,.tsx -d es --source-maps && npm run build:css",
    "build": "cross-env BABEL_ENV=umd webpack --config webpack/webpack.prod.js --mode production",
    "dev": "webpack-dev-server --config webpack/webpack.dev.js --open"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:wangzi6224/calendar-ts.git"
  },
  "keywords": [
    "react",
    "typescript",
    "calendar"
  ],
  "author": "wangzi6224",
  "license": "ISC",
  "homepage": "https://github.com/wangzi6224/calendar-ts/blob/master/README.md",
  "bugs": {
    "url": "https://github.com/wangzi6224/calendar-ts/issues"
  },
  "sideEffects": [
    "dist/*",
    "es/**/style/*",
    "lib/**/style/*",
    "*.less"
  ],
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.17.12",
    "@babel/plugin-proposal-decorators": "^7.17.12",
    "@babel/plugin-transform-runtime": "^7.17.12",
    "@babel/preset-env": "^7.17.12",
    "@babel/preset-react": "^7.17.12",
    "@babel/preset-typescript": "^7.17.12",
    "@types/moment": "^2.13.0",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.4",
    "autoprefixer": "^10.4.7",
    "babel-loader": "^8.2.5",
    "babel-plugin-module-alias": "^1.6.0",
    "clean-webpack-plugin": "^4.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^4.0.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^8.0.0",
    "gulp-clean-css": "^4.3.0",
    "gulp-concat": "^2.6.1",
    "gulp-filesize": "^0.0.6",
    "gulp-less": "^5.0.0",
    "gulp-postcss": "^9.0.1",
    "gulp-rename": "^2.0.0",
    "gulp-replace": "^1.1.3",
    "gulp-sourcemaps": "^3.0.0",
    "html-webpack-plugin": "^5.5.0",
    "less-loader": "^11.0.0",
    "mini-css-extract-plugin": "^2.6.0",
    "postcss-loader": "^7.0.0",
    "postcss-preset-env": "^7.5.0",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.1",
    "ts-loader": "^9.3.0",
    "typescript-plugin-css-modules": "^3.4.0",
    "webpack": "^5.72.1",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.9.0",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "babel-plugin-module-resolver": "^4.1.0",
    "moment": "^2.29.3",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "typescript": "^4.6.4"
  }
}
