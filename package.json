{
  "name": "calendar-react-ts",
  "version": "0.0.5",
  "description": "a calendar component by react with typescript",
  "main": "es/index.js",
  "scripts": {
    "start": "npm run dev",
    "build:commonjs": "rimraf lib && cross-env BABEL_ENV=commonjs babel src --extensions .ts,.tsx -d lib --source-maps",
    "build:es": "rimraf es && cross-env BABEL_ENV=es babel src --extensions .ts,.tsx -d es --source-maps",
    "dev": "webpack-dev-server --config webpack/webpack.dev.js --open",
    "build": "webpack --config webpack/webpack.prod.js --mode production",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:wangzi6224/calendar-ts.git"
  },
  "keywords": [
    "react",
    "typescript",
    "calendar"
  ],
  "author": "wangzi6224",
  "license": "ISC",
  "homepage": "https://github.com/wangzi6224/calendar-ts/blob/master/README.md",
  "bugs": {
    "url": "https://github.com/wangzi6224/calendar-ts/issues"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.17.12",
    "@babel/plugin-proposal-decorators": "^7.17.12",
    "@babel/plugin-transform-runtime": "^7.17.12",
    "@babel/preset-env": "^7.17.12",
    "@babel/preset-react": "^7.17.12",
    "@babel/preset-typescript": "^7.17.12",
    "@types/moment": "^2.13.0",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.4",
    "autoprefixer": "^10.4.7",
    "babel-loader": "^8.2.5",
    "clean-webpack-plugin": "^4.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^4.0.0",
    "html-webpack-plugin": "^5.5.0",
    "less-loader": "^11.0.0",
    "mini-css-extract-plugin": "^2.6.0",
    "postcss-loader": "^7.0.0",
    "postcss-preset-env": "^7.5.0",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.1",
    "ts-loader": "^9.3.0",
    "typescript-plugin-css-modules": "^3.4.0",
    "webpack": "^5.72.1",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.9.0",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "moment": "^2.29.3",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "typescript": "^4.6.4"
  }
}
